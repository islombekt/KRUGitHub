
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="title-container">
    <h2 class="text-info">Фойдаланувчилар рўйхати</h2>
    <div class="page-title-subheading">
        Бу ерда фойдаланувчиларни бошқариш мумкин
    </div>
    <a class="btn btn-default" asp-area="Identity" asp-page="/Account/Register">
        <i class="fas fa-plus mr-2"></i>Фойдаланувчи Қўшиш
    </a>
</div>

<div class="data-table-area">
    <div class="main-card mb-3 card">
        <div class="card-body">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="data-table-list">
                    <div class="basic-tb-hd">
                        <div class="row">
                            <div class="table-responsive"> 
                                <table id="data-table-basic" class="table table-hover" style="width:100%">
                                    <thead>
                                        <tr>
                                            <th style="width:10px">Исми</th>
                                            <th>Фамилияси</th>
                                            <th>Почтаси</th>
                                            <th>Тел. рақами</th>
                                            <th>Address</th>
                                            <th style="width:10px">Роли</th>
                                            <th></th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tfoot>
                                        <tr>
                                            <th style="width:10px">Исми</th>
                                            <th>Фамилияси</th>
                                            <th>Почтаси</th>
                                            <th>Тел. рақами</th>
                                            <th>Address</th>
                                            <th style="width:10px">Роли</th>
                                            <th></th>
                                            <th></th>
                                        </tr>
                                    </tfoot>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


